@{
    Page.Title = "Contact";
}
<div class="row">
    <div class="col-md-4 col-md-offset-1 js hidden-sm hidden-xs">
        <h2>Contact form</h2>
        <form id="contact-form" class="form-horizontal" role="form" method="post">
            <p>Fill out the form below and I'll get back to you soon... probably.</p>
            <div class="form-group">
                <input type="text" class="form-control" name="name" id="name" maxlength="150" placeholder="Your Name">
            </div>

            <div class="form-group">
                <input type="email" class="form-control" name="email" id="email" maxlength="150" placeholder="Your Email Address">
            </div>

            <div class="form-group">
                <textarea class="form-control" rows="8" name="message" id="message" placeholder="Message (no support questions or issues, see below)"></textarea>
            </div>
            <p class="text-center text-muted disclaimer">
                Please note that I will not answer any support questions or issues related to my code and/or projects per e-mail (<a href="https://xkcd.com/979/">why?</a>). If you're looking for support on any of my <a href="/static/code">code and/or projects</a>, please open an issue at the respective project's site (probably <a href="https://github.com/RobThree?tab=repositories">GitHub</a>, and <a href="https://help.github.com/articles/creating-an-issue/">here's how to do it</a>).
            </p>
            <div class="form-group">
                <input type="text" name="schd" value="" maxlength="150" class="hd">
                <button type="submit" class="btn btn-default form-control">Send</button>
            </div>
        </form>
    </div>
    <div class="col-md-6 col-md-offset-1">
